pipeline{
    agent {
        label 'dev-slave'
    }
    options {
        buildDiscarder logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '', daysToKeepStr: '30', numToKeepStr: '2')
    }
    stages{
        stage("Checkout SCM"){
            steps{
                git branch: 'main', url: 'https://github.com/RAVIREDDY217/Sorting.git'
            }
        }
    }
}
